{
  "name": "minimize",
  "version": "1.2.0",
  "description": "Minimize HTML",
  "main": "./lib/minimize",
  "scripts": {
    "test": "NODE_ENV=test mocha $(find test -name '*.test.js')",
    "coverage": "NODE_ENV=test istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true && rm -rf ./coverage"
  },
  "dependencies": {
    "argh": "0.1.x",
    "cli-color": "0.3.x",
    "htmlparser2": "3.8.x",
    "utile": "0.3.x"
  },
  "devDependencies": {
    "coveralls": "2.11.x",
    "mocha": "2.0.x",
    "mocha-lcov-reporter": "0.0.x",
    "chai": "1.10.x",
    "istanbul": "0.3.x",
    "sinon": "1.12.x",
    "sinon-chai": "2.6.x",
    "pre-commit": "0.0.x"
  },
  "repository": "git@github.com:Moveo/minimize.git",
  "keywords": [
    "minify",
    "minimize",
    "HTML"
  ],
